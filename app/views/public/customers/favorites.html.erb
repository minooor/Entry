<div>お気に入りチーム一覧</div>
<table class="table">
  <thead>
    <tr>
      <th>チーム名</th>
      <th>活動拠点</th>
      <th>競技名</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @favorite_profiles.each do |profile|%>
    <tr>
      <td>
        <%= link_to profile_path(profile) do %>
        <%= profile.customer.name %>
        <% end %>
      </td>
      <td>
        <%= profile.address %>
      </td>
      <td>
        <%= profile.category %>
      </td>
      <td>
        
      </td>
    </tr>
    <% end %>
  </tbody>
</table>