<%= search_form_for q, url: admin_posts_path do |f| %>
  <%= f.search_field :customer_name_cont, placeholder: "名前" %>
  <%= f.search_field :category_cont, placeholder: "競技名" %>
  <%= f.submit "検索", class: "btn-sm btn-primary" %>
  <%= link_to 'クリア', request.path, class: "btn-sm btn-secondary" %>
<% end %>